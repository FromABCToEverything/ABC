<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.example.demo.entity.NoteEntity">
        <table name="note" schema="test" catalog=""/>
        <attributes>
            <id name="noteId">
                <column name="note_id" precision="-1"/>
            </id>
            <basic name="creatorId">
                <column name="creator_id"/>
            </basic>
            <basic name="creator">
                <column name="creator"/>
            </basic>
            <basic name="avatarUrl">
                <column name="avatar_url"/>
            </basic>
            <basic name="title">
                <column name="title"/>
            </basic>
            <basic name="content">
                <column name="content"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

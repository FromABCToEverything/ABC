<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.example.demo.entity.QuestionEntity">
        <table name="question" schema="test" catalog=""/>
        <attributes>
            <id name="questionId">
                <column name="question_id" precision="-1"/>
            </id>
            <basic name="creatorId">
                <column name="creator_id"/>
            </basic>
            <basic name="type">
                <column name="type" length="1"/>
            </basic>
            <basic name="pointId">
                <column name="point_id" precision="-1"/>
            </basic>
            <basic name="stem">
                <column name="stem" length="511"/>
            </basic>
            <basic name="choices">
                <column name="choices" length="511"/>
            </basic>
            <basic name="answer">
                <column name="answer" length="1"/>
            </basic>
            <basic name="explanation">
                <column name="explanation" length="511"/>
            </basic>
            <basic name="lastEditTime">
                <column name="last_edit_time" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

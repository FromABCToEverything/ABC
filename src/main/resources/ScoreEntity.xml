<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.example.demo.entity.ScoreEntity">
        <table name="score" schema="test" catalog=""/>
        <attributes>
            <basic name="type">
                <column name="type" length="1"/>
            </basic>
            <basic name="id">
                <column name="id" precision="-1"/>
            </basic>
            <basic name="totalScore">
                <column name="total_score" precision="-1"/>
            </basic>
            <basic name="scoredTimes">
                <column name="scored_times" precision="-1"/>
            </basic>
            <basic name="avgScore">
                <column name="avg_score" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
